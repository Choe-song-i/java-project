
public class Ex20 {

	public static void main(String[] args) {
		int a=10;
		int b=4;
		
		System.out.printf("%d + %d= %d\n",a,b,a+b);
		//%d출력행태는 십진수 정수, \n은 줄 바꿈
		System.out.printf("%d - %d= %d\n",a,b,a-b);
		System.out.printf("%d * %d= %d\n",a,b,a*b);
		System.out.printf("%d / %d= %d\n",a,b,a/b);
		//정수 숫자를 나눗셈하면 몫만 구함.

	}

}
